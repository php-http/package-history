{
    "name": "php-http/common",
    "description": "Common implementations of HTTP related interfaces",
    "license": "MIT",
    "keywords": ["http", "adapter", "client", "common"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "php-http/message-factory": "dev-master"
    },
    "require-dev": {
        "php-http/message-decorator": "dev-master",
        "zendframework/zend-diactoros": "~1.0",
        "guzzlehttp/psr7": "~1.0",
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "suggest": {
        "php-http/message-decorator": "Used with decorated messages",
        "zendframework/zend-diactoros": "Used with Diactoros MessageFactory",
        "guzzlehttp/psr7": "Used with Guzzle PSR-7 MessageFactory"
    },
    "autoload": {
        "psr-4": {
            "Http\\Common\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
