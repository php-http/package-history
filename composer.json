{
    "name": "php-http/encoding",
    "description": "PSR 7 Stream for transfer / content encoding headers (chunked, deflate, gzip, ...)",
    "license": "MIT",
    "keywords": ["encoding", "http", "stream"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "joel.wurtz@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-zlib": "*",
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1.0",
        "clue/stream-filter": "~1.3"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4-alpha",
        "henrikbjorn/phpspec-code-coverage" : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Http\\Encoding\\": "src/"
        }
    },
    "scripts": {
        "test": "",
        "test-ci": ""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
