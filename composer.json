{
    "name": "php-http/adapter-core",
    "description": "HTTP Adapter default implementations and internal classes",
    "license": "MIT",
    "keywords": ["http", "adapter"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "php-http/adapter-common": "dev-master",
        "php-http/adapter-internal": "dev-master",
        "php-http/message-decorator": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Http\\Adapter\\Core\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
